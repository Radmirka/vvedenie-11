#include <iostream>
#include <clocale>
	using namespace std;
	int main()
	{
	setlocale(LC_ALL,"Rus");
	    int m,l;
	    int i=2;
	    cout<<"Введите число"<<endl;
	    cin>>m;
	    int a=0;
	    int b=1;
	    while (b!=m)
	    {
	       l=b;
	       b=a+b;
	       a=l;
	       i++;
	    }
	    cout<<"K="<<i-1<<endl;
	}
